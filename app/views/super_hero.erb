<h1>Create a Team and Heroes!</h1>
<form action ="/teams" method="post">
    <label for="team_name">Team Name</label><br>
    <input id="team_name" type="text" name="team[name]"><br>
    <label for="team_motto">Team Motto</label><br>
    <input id="team_motto" type="text" name="team[motto]"><br>

    <label for="member1_name">Member 1 Name</label><br>
    <input id="member1_name" type="text" id="member1_name" name="team[members][][name]"><br>
    <label for="member1_power">Member 1 Power</label><br>
    <input id="member1_power" type="text" id="member1_power" name="team[members][][power]"><br>
    <label for="member1_bio">Member 1 Bio</label><br>
    <input id="member1_bio" type="text" id="member1_bio" name="team[members][][bio]"><br>

    <label for="member2_name">Member 2 Name</label><br>
    <input id="member2_name" type="text" id="member2_name" name="team[members][][name]"><br>
    <label for="member2_power">Member 2 Power</label><br>
    <input id="member2_power" type="text" id="member2_power" name="team[members][][power]"><br>
    <label for="member2_bio">Member 2 Bio</label><br>
    <input id="member2_bio" type="text" id="member2_bio" name="team[members][][bio]"><br>

    <label for="member3_name">Member 3 Name</label><br>
    <input id="member3_name" type="text" id="member3_name" name="team[members][][name]"><br>
    <label for="member3_power">Member 3 Power</label><br>
    <input id="member3_power" type="text" id="member3_power" name="team[members][][power]"><br>
    <label for="member3_bio">Member 3 Bio</label><br>
    <input id="member3_bio" type="text" id="member3_bio" name="team[members][][bio]"><br>

    <input type="submit" value="submit">
</form>
</form>